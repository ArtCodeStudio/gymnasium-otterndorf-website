{
  "collectionName": "components_podcast_chapters",
  "info": {
    "name": "Chapters",
    "icon": "bars",
    "description": ""
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "href": {
      "model": "navigation-link"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "start": {
      "type": "string",
      "required": true,
      "regex": "^([0-1]?[0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9].[0-9][0-9][0-9]$",
      "default": "00:00:00.000"
    }
  }
}
