{
  "name": "@gymott/nest",
  "version": "0.0.1",
  "description": "",
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=development nest start --debug",
    "start:prod": "NODE_ENV=production node --expose-gc dist/main",
    "watch": "NODE_ENV=development nest start --watch --debug",
    "watch:debug": "NODE_ENV=development nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:todo": "jest",
    "test": "echo \"TODO: implement tests\" && exit 1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@gymott/common": "workspace:*",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/platform-express": "^7.6.17",
    "@ribajs/events": "workspace:*",
    "@ribajs/nest-lunr": "workspace:*",
    "@ribajs/nest-theme": "workspace:*",
    "@ribajs/node-graphql-client": "workspace:*",
    "@ribajs/ssr": "workspace:*",
    "@types/consolidate": "^0.14.0",
    "@types/express": "^4.17.12",
    "@types/ical": "^0.6.2",
    "@types/jsdom": "^16.2.11",
    "cache-manager": "^3.4.3",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "find-root": "^1.1.0",
    "glob": "^7.1.7",
    "ical": "^0.8.0",
    "jsdom": "^16.6.0",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.4.0",
    "node-fetch": "^2.6.1",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "remark": "^13.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "strip-markdown": "^4.0.0",
    "tinyliquid": "^0.2.34",
    "twig": "^1.15.4",
    "whatwg-encoding": "^1.0.5",
    "yaml": "next"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/lunr": "^2.3.3",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
