{
  "name": "riba-nest-projects",
  "private": true,
  "author": "Art+Code Studio (Pascal Garber) <pascal@artandcode.studio>",
  "scripts": {
    "postinstall": "yarn run install:outside",
    "install:outside": "cd apps/gymott/strapi && NODE_OPTIONS='' npm install",
    "build": "yarn workspaces foreach run build && yarn run build:outside",
    "build:outside": "cd apps/gymott/strapi NODE_OPTIONS='' npm run build",
    "start:local": "yarn workspace @manager/nest run start:local",
    "start:prod": "yarn workspace @manager/nest run start:prod",
    "start:dev": "yarn workspace @manager/nest run start:dev",
    "watch:local": "yarn workspace @manager/nest run watch:local",
    "watch:prod": "yarn workspace @manager/nest run watch:prod",
    "watch:dev": "yarn workspace @manager/nest run watch:dev"
  },
  "workspaces": [
    "apps/*/*",
    "!apps/*/strapi"
  ],
  "resolutions": {
    "@types/express-serve-static-core": "latest",
    "@types/node": "latest",
    "@ribajs/nest-theme": "portal:../riba/backend/nest-theme",
    "@ribajs/demo-router-view": "portal:../riba/examples/router-view",
    "@ribajs/eslint-config": "portal:../riba/infra/eslint-config",
