{
  "kind": "collectionType",
  "collectionName": "blog_entries",
  "info": {
    "name": "Blog Entry"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 80
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "content.text",
        "content.image"
      ]
    },
    "blog_category": {
      "via": "blog_entries",
      "model": "blog-category"
    }
  }
}
