{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.image",
        "content.text"
      ]
    },
    "assets": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "calendar_key": {
      "type": "string"
    },
    "blog_entries": {
      "collection": "blog-entry",
      "via": "pages",
      "dominant": true
    },
    "navigation_sections": {
      "via": "pages",
      "collection": "navigation-section"
    }
  }
}
